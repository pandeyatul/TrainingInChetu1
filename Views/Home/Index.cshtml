 @model IEnumerable<TrainingInChetu.Models.ViewModels.CustomerViewModel>
     <h4 style="text-align:center">All Customers List</h4>
     <div class="container">
         <a class="btn btn-success float-right" asp-action="Create" asp-controller="Home" >Create</a>
         <table class="table table-bordered">
             <tr>
                 <th>Customer ID</th>
                 <th>Name</th>
                 <th>Contact No</th>
                 <th>Email ID</th>
                 <th>Address</th>
                 <th>Location</th>
                 <th>Actions</th>
             </tr>
             @foreach (var cd in Model)
             {
         <tr>
             <td>@cd.Id</td>
             <td>@cd.Name</td>
             <td>@cd.Contactno</td>
             <td>@cd.EmailId</td>
             <td>@cd.Address</td>
             <td>@cd.Location</td>
             <td>
                 <a class="btn btn-danger" asp-action="Delete" asp-controller="Home" asp-route-id="@cd.Id" onclick="return
                  confirm('Are you sure to delete this data?') ">Delete</a>||
                 <a class="btn btn-success" asp-action="Edit" asp-route-id="@cd.Id" asp-controller="Home">Edit</a>
             </td>
         </tr>
             }

         </table>
     </div>